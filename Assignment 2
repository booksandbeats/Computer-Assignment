//C program to print multiplication table of any number.

#include <stdio.h>

int main()
{
    int n, a, b, c, d, e, f, g, h, i, j;
    printf("Enter any number: ");
    scanf("%d", &n);
    a = n*1;
    b = n*2;
    c = n*3;
    d = n*4;
    e = n*5;
    f = n*6;
    g = n*7;
    h = n*8;
    i = n*9;
    j = n*10;
    printf("Multiplication table of given number: \n");
    printf("%d x 1 = %d\n", n, a);
    printf("%d x 2 = %d\n", n, b);
    printf("%d x 3 = %d\n", n, c);
    printf("%d x 4 = %d\n", n, d);
    printf("%d x 5 = %d\n", n, e);
    printf("%d x 6 = %d\n", n, f);
    printf("%d x 7 = %d\n", n, g);
    printf("%d x 8 = %d\n", n, h);
    printf("%d x 9 = %d\n", n, i);
    printf("%d x 10 = %d", n, j);
    
    return 0;
}
